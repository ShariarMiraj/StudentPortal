@{
}

@model StudentPortal.Web.Models.AddDiscussionForumViewModel

<h1> DiscussionForum</h1>

<form method="post">
    
    <div class="mt-3">
        <label class="form-label">ForumName</label>
        <input type="text" class="form-control" asp-for="ForumName" />
    </div>

    <div class="mt-3">
        <label class="form-label">Description</label>
        <input type="text" class="form-control" asp-for="Description" />
    </div>


    <div class="mt-3">
        <label class="form-label">DateTime</label>
        <input type="text" class="form-control" asp-for="DateTime"/>
    </div>



    <div class="mt-3">
        <label class="form-label">Creator</label>
        <input type="text" class="form-control" asp-for="Creator" />
    </div>

    <div class="mt-3">
        <button type="submit" class="btn btn-primary">Save</button>
    </div>
</form>

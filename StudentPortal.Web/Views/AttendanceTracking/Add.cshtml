@{
}

@model StudentPortal.Web.Models.AttendanceTrackingViewModel

<h1>AttendanceTracking</h1>


<form method="post">

    <div class="mt-3">
        <label class="form-label">Session</label>
        <input type="text" class="form-control" asp-for="Session" />

    </div>

    <div class="mt-3">
        <label class="form-label">StudentID</label>
        <input type="text" class="form-control" asp-for="StudentID" />
    </div>

    <div class="mt-3">
        <label class="form-label">Timestamp</label>
        <input type="text" class="form-control" asp-for="Timestamp" />

    </div>

    <div class="mt-3">
        <label class="form-label">LateArrivalTime</label>
        <input type="text" class="form-control" asp-for="LateArrivalTime" />
    </div>

    <div class="mt-3">
        <button type="submit" class="btn btn-primary">Save</button>

    </div>
</form>

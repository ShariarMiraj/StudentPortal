@{
}

@model List<StudentPortal.Web.Models.Entities.Fee>


<h1>List</h1>
<table class="table">
    <thead>
        <tr>
            <th>FeeId</th>
            <th>CourseID</th>
            <th>FeeType</th>
            <th>Amount</th>
            <th>LateFee</th>
            <th>PaymentDate</th>

            <th></th>

        </tr>
    </thead>
    <tbody>
        @foreach ( var fee in Model)
        {
            <tr>
                <td>@fee.FeeId</td>
                <td>@fee.CourseID</td>
                <td>@fee.FeeType</td>
                <td>@fee.Amount</td>
                <td>@fee.LateFee</td>
                <td>@fee.PaymentDate</td>

                <td>
                    <a class="btn btn-primary">Edit</a>
                </td>
            </tr>
        }
    </tbody>
</table>
@{
}

@model StudentPortal.Web.Models.AddFeeViewModel

<h1>Fee</h1>

<form method="post">

    <div class="mt-3">
        <label class="form-label">CourseID</label>
        <input type="text" class="form-control" asp-for="CourseID" />
    </div>

    <div class="mt-3">
        <label class="form-label"> FeeType</label>
        <input type="text" class="form-control" asp-for="FeeType" />
    </div>

    <div class="mt-3">
        <label class="form-label">Amount </label>
        <input type="text" class="form-control" asp-for="Amount" />
    </div>


    <div class="mt-3">
        <label class="form-label">LateFee</label>
        <input type="text" class="form-control" asp-for="LateFee" />
    </div>

    <div class="mt-3">
        <label class="form-label">PaymentDate </label>
        <input type="text" class="form-control" asp-for="PaymentDate"/>
    </div>

    <div class="mt-3">
        <button type="submit" class="btn btn-primary">save</button>
    </div>
</form>

@{
}

@model List<StudentPortal.Web.Models.Entities.Teacher>

<h1>All Teacher</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Category</th>
            <th>Phone</th>
            <th>Email</th>
            <th>  </th>
        </tr>
    </thead>
    <tbody>
        @foreach ( var teacher in Model)
        {
            <tr>
                <td>@teacher.Id</td>
                <td>@teacher.Name</td>
                <td>@teacher.Category</td>
                <td>@teacher.Phone</td>
                <td>@teacher.Email</td>

                <td>
                    <a class="btn btn-primary" asp-controller="Teacher" asp-action="Edit" asp-route-id="@teacher.Id">Edit</a>
                </td>
            </tr>
        }
    </tbody>
</table>
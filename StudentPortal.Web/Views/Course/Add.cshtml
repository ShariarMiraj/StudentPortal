@{
}

@model StudentPortal.Web.Models.AddCourseViewModel

<h1>Course</h1>


<form method="post">


    <div class="mt-3">
        <label class="form-label">Name</label>
        <input type="text" class="form-control" asp-for="Name" />
    </div>


    <div class="mt-3">
        <label class="form-label">Description</label>
        <input type="text" class="form-control" asp-for="Description" />
    </div>

    <div class="mt-3">
        <label class="form-label">CreatedDate</label>
        <input type="text" class="form-control" asp-for="CreatedDate" />
    </div>

    <div class="mt-3">
        <label class="form-label">Department</label>
        <input type="text" class="form-control" asp-for="Department" />
    </div>

    <div class="mt-3">
        <label class="form-label">EnrolledStudents </label>
        <input type="text" class="form-control" asp-for="EnrolledStudents" />
    </div>


    <div class="mt-3">
        <button type="submit" class="btn btn-primary">Save</button>

    </div>

</form>
